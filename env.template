# MedGemma Audio Pipeline Environment Variables
# Copy this file to .env and fill in your values:
#   cp env.template .env

# Required: HuggingFace token for downloading MedGemma
HF_TOKEN=your_huggingface_token_here

# Optional: Weights & Biases API key for experiment tracking
WANDB_API_KEY=

# Optional: Set to "offline" to disable wandb logging
WANDB_MODE=online
